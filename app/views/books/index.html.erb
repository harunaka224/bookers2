<main>
    <p id="notice"></p>
    <div class="container px-5 px-sm-0">
        <div class="low">
            <div class="col-md-3">
                <h2>User info</h2>
                <img >
                <table class="table">
                    <tbody>
                        <tr></tr>
                        <tr>
                            <th>name</th>
                            <th></th>
                        </tr>
                        <tr>
                            <th>introduction</th>
                            <th></th>
                        </tr>
                        <tr></tr>
                    </tbody>
                </table>
                <div class="low">
                    <a class="btn btn-outline-secondary btn-block edit_user_id" href="/users/:id/edit">
                      <i class="fas fa-user-cog"></i>
                    </a>
                </div>
                <h2 class="mt-3">New book</h2>
                <%= form_with model:@book do |f| %>
                <h4>Title</h4>
                <%= f.text_field :title %>
                <h4>Opinion</h4>
                <%= f.text_area :body %>
                <%= f.submit'Create Box' %>
                <% end %>
            </div>
        </div>
    </div>
</main>


